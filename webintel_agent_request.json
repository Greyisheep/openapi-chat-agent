{
  "name": "WebIntel Blog Agent",
  "openapi_spec": {
    "openapi": "3.0.0",
    "info": {
      "title": "WebIntel API",
      "description": "WebIntel Blog API for managing blog posts and content",
      "version": "1.0.0"
    },
    "servers": [{"url": "https://api.webintel.co"}],
    "paths": {
      "/api/v1/blog/public": {
        "get": {
          "summary": "Get public blogs",
          "description": "Retrieve all public blog posts",
          "operationId": "getPublicBlogs",
          "parameters": [
            {
              "name": "skip",
              "in": "query",
              "description": "Number of records to skip",
              "required": false,
              "schema": {
                "type": "integer",
                "default": 0
              }
            },
            {
              "name": "limit",
              "in": "query", 
              "description": "Number of records to return",
              "required": false,
              "schema": {
                "type": "integer",
                "default": 100
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "data": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "id": {"type": "string"},
                            "title": {"type": "string"},
                            "content": {"type": "string"},
                            "author": {"type": "string"},
                            "created_at": {"type": "string", "format": "date-time"},
                            "updated_at": {"type": "string", "format": "date-time"}
                          }
                        }
                      },
                      "total": {"type": "integer"},
                      "skip": {"type": "integer"},
                      "limit": {"type": "integer"}
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/v1/blog/{blog_id}": {
        "get": {
          "summary": "Get blog by ID",
          "description": "Retrieve a specific blog post by its ID",
          "operationId": "getBlogById",
          "parameters": [
            {
              "name": "blog_id",
              "in": "path",
              "required": true,
              "description": "The ID of the blog post",
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "id": {"type": "string"},
                      "title": {"type": "string"},
                      "content": {"type": "string"},
                      "author": {"type": "string"},
                      "created_at": {"type": "string", "format": "date-time"},
                      "updated_at": {"type": "string", "format": "date-time"}
                    }
                  }
                }
              }
            },
            "404": {
              "description": "Blog not found"
            }
          }
        }
      }
    }
  },
  "user_api_key": "",
  "api_base_url": "https://api.webintel.co",
  "user_instructions": "You are a helpful assistant for the WebIntel blog platform. You can help users browse public blog posts, view specific blog content, and provide information about the blog articles. Be friendly and informative when interacting with users about the blog content."
}

