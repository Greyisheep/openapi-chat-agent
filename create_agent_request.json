{
  "name": "GitHub Agent",
  "openapi_spec": {
    "openapi": "3.0.0",
    "info": {
      "title": "GitHub API",
      "version": "1.0.0",
      "description": "GitHub REST API for managing repositories and users"
    },
    "servers": [
      {
        "url": "https://api.github.com",
        "description": "GitHub API server"
      }
    ],
    "paths": {
      "/user": {
        "get": {
          "summary": "Get authenticated user",
          "description": "Get the authenticated user's profile",
          "operationId": "getUser",
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "integer"
                      },
                      "login": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "email": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/user/repos": {
        "get": {
          "summary": "List repositories for authenticated user",
          "description": "List repositories that the authenticated user has explicit permission to access",
          "operationId": "listUserRepos",
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "integer"
                        },
                        "name": {
                          "type": "string"
                        },
                        "full_name": {
                          "type": "string"
                        },
                        "description": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "user_api_key": "ghp_test123",
  "api_base_url": "https://api.github.com",
  "user_instructions": "You are a helpful assistant for managing GitHub repositories and users. You can help users get their profile information and list their repositories."
}

